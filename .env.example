# Copy this file to `.env` and fill in the required values.

############################
# LLM PROVIDER CONFIG
############################

# OpenAI (recommended if you have credits)
# Uncomment and fill if you want to use OpenAI:
 AUDITOR_LLM_PROVIDER=openai
 OPENAI_API_KEY=your-openai-key-here
 AUDITOR_LLM_MODEL=gpt-4o

# Groq (good free-tier option)
# Uncomment and fill if you want to use Groq:
 AUDITOR_LLM_PROVIDER=groq
 GROQ_API_KEY=your-groq-key-here
 AUDITOR_LLM_MODEL=llama-3.1-70b-versatile

# Google Gemini (free-tier option)
# Uncomment and fill if you want to use Gemini:
 AUDITOR_LLM_PROVIDER=gemini
 GOOGLE_API_KEY=your-gemini-key-here
 AUDITOR_LLM_MODEL=gemini-1.5-flash

############################
# LangSmith / tracing (optional)
############################

LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your-langsmith-key-here

